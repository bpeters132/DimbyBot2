{
  "name": "dimbybot2",
  "version": "0.1.4",
  "type": "module",
  "description": "A discord bot for friends!",
  "main": "./src/index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "nodemon ./src/index.js",
    "deployGlobal": "node ./src/deploy/deployGlobal.js",
    "destroyGlobal": "node ./src/deploy/destroyGlobal.js",
    "deployGuild": "node ./src/deploy/deployGuild.js",
    "destroyGuild": "node ./src/deploy/destroyGuild.js",
    "lint": "eslint --cache --ext .js,.cjs,.mjs src/ Lavalink/ docs/suggestions",
    "lint:fix": "yarn lint --fix",
    "format": "prettier \"**/*.{js,cjs,mjs,json,md,yml,yaml}\"",
    "format:check": "yarn format --check",
    "markdownlint": "markdownlint \"docs/**/*.md\" README.md",
    "lint:all": "yarn lint && yarn format:check && yarn markdownlint",
    "hooks:setup": "husky install && npx husky set .husky/pre-commit 'yarn lint-staged'",
    "hooks:remove": "rm -rf .husky"
  },
  "lint-staged": {
    "*.{js,cjs,mjs}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "colors": "^1.4.0",
    "date-fns": "^4.1.0",
    "discord.js": "^14.6.0",
    "dotenv": "^17.2.0",
    "ffmpeg-static": "^5.2.0",
    "lavalink-client": "^2.5.0",
    "nodemailer": "^7.0.5",
    "sodium": "^3.0.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "eslint": "^9.31.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-promise": "^7.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "markdownlint-cli": "^0.44.0",
    "nodemon": "^3.1.9",
    "prettier": "3.6.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "nodemonConfig": {
    "ignore": [
      "storage/*",
      "logs/*"
    ],
    "ext": "js,json"
  }
}
